<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myungjin.withusplanet"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="28" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    />
    <application
        android:allowBackup="true"
        android:appComponentFactory="android.support.v4.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Server Analyzer"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.myungjin.withusplanet.View.IntroActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.myungjin.withusplanet.View.Login_Activity"
            android:screenOrientation="portrait" />
        <!--
            <activity
            android:name=".aaaaa"
            android:label="@string/title_activity_aaaaa" />
        -->
        <activity
            android:name="com.example.myungjin.withusplanet.View.ViewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.myungjin.withusplanet.View.FindingPW"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.myungjin.withusplanet.View.UserInfo"
            android:screenOrientation="portrait"
            android:theme="@style/Transparent" />
        <activity
            android:name="com.example.myungjin.withusplanet.View.ChangePW"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.myungjin.withusplanet.View.LeaveAccount"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.myungjin.withusplanet.View.SignupActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.myungjin.withusplanet.View.User_profile"
            android:screenOrientation="portrait" />
        <activity android:name="com.example.myungjin.withusplanet.View.SelectSimAP" />
        <activity android:name="com.example.myungjin.withusplanet.View.SimViewActivity" />
        <activity
            android:name="com.gun0912.tedpermission.TedPermissionActivity"
            android:theme="@style/Theme.Transparent.Permission" />
    </application>

</manifest>