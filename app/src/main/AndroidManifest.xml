<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myungjin.withusplanet">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Server Analyzer"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".View.Login_Activity"
            android:screenOrientation="portrait" />
        <!--
            <activity
            android:name=".aaaaa"
            android:label="@string/title_activity_aaaaa" />
        -->
        <activity
            android:name=".View.ViewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".View.FindingPW"
            android:screenOrientation="portrait" />
        <activity
            android:name=".View.UserInfo"
            android:screenOrientation="portrait"
            android:theme="@style/Transparent" />
        <activity
            android:name=".View.ChangePW"
            android:screenOrientation="portrait" />
        <activity
            android:name=".View.LeaveAccount"
            android:screenOrientation="portrait" />
        <activity
            android:name=".View.SignupActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".View.User_profile"
            android:screenOrientation="portrait" />
        <activity android:name=".View.SelectSimAP" />
        <activity android:name=".View.SimViewActivity"></activity>
    </application>

</manifest>